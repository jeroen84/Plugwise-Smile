	<template id='1b027c50243c4c639f28eab6409d822d' tag="zone_setpoint_and_state_based_on_preset">
		<name>Zone preset template</name>
		<description>Template for actuating thermostats and relays on a preset basis.</description>
		<single_actor>true</single_actor>
		<created_date>2019-01-31T12:18:31.874+01:00</created_date>
		<modified_date>2020-05-12T01:22:04.463+02:00</modified_date>
		<deleted_date></deleted_date>
		<objects>
			<object name="zone" type="Location"/>
		</objects>
		<parameters>
			<parameter name="preset" source="zone.preset" type="Preset"/>
		</parameters>
		<results>
			<result name="cooling_setpoint" type="Number" action="zone.iterateFunctionalities('ThermostatFunctionality')().setSetpoint(null, null, cooling_setpoint)"/>
			<result name="domestic_hot_water_state" type="OnOff" action="zone.iterateFunctionalities('ToggleFunctionality', 'domestic_hot_water_state')().state = domestic_hot_water_state"/>
			<result name="heating_setpoint" type="Number" action="zone.iterateFunctionalities('ThermostatFunctionality')().setSetpoint(null, heating_setpoint, null)"/>
			<result name="state" type="OnOff" action="zone.iterateFunctionalities('RelayFunctionality')().state = state"/>
		</results>
	</template>
